{
  "name": "styledux",
  "description": "A new approach of isomorphic css resolution with webpack, css-loader(css-modules) powered by redux.",
  "version": "0.2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "sideEffects": false,
  "directories": {
    "test": "jest"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "loader.js"
  ],
  "scripts": {
    "clean": "rimraf lib es loader.js",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -f cjs -o lib/index.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -f es -o es/index.js",
    "build:loader": "babel src/loader.js -o loader.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:loader",
    "lint": "eslint src test",
    "posttest": "npm run lint",
    "prepare": "npm run clean && npm run test && npm run build",
    "test": "npm run lint",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "style",
    "react",
    "redux",
    "css",
    "isomorphic",
    "universal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openameba/styledux.git"
  },
  "author": "Bin Hou <houbin217jz@gmail.com> (https://github.com/kouhin)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=15"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/node": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/polyfill": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.5",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^22.4.3",
    "jsdom": "^11.10.0",
    "lerna": "^2.9.1",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "react": "^16.3.2",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "loader-utils": "^1.1.0",
    "mitt": "^1.1.3",
    "prop-types": "^15.6.1",
    "react-broadcast": "^0.7.0",
    "redux": "^4.0.0"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.js)$"
  }
}
