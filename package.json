{
  "name": "styledux",
  "description": "A new approach of isomorphic css resolution with webpack, css-loader(css-modules) powered by redux.",
  "version": "0.3.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "sideEffects": false,
  "directories": {
    "test": "jest"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "loader.js"
  ],
  "scripts": {
    "clean": "rimraf lib es loader.js",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -f cjs -o lib/index.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -f es -o es/index.js",
    "build:loader": "babel src/loader.js -o loader.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:loader",
    "lint": "eslint src",
    "posttest": "npm run lint",
    "prepare": "npm run clean && npm run test && npm run build",
    "test": "npm run lint",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "style",
    "react",
    "redux",
    "css",
    "isomorphic",
    "universal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openameba/styledux.git"
  },
  "author": "Bin Hou <houbin217jz@gmail.com> (https://github.com/kouhin)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react": "^16.8.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "dependencies": {
    "loader-utils": "^1.2.3",
    "mitt": "^1.1.3",
    "prop-types": "^15.7.2",
    "redux": "^4.0.1"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.js)$"
  }
}
